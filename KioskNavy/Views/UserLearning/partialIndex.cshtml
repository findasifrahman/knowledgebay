@model IEnumerable<KioskNavy.Models.IndexForLearning>
<style>
    body {
        background-color: #ffffff;
        background-image: url(../../img/36.jpg);
        background-repeat: no-repeat;
        background-size: cover;

    }
</style>
@section Scripts
{
    <script type="text/javascript">
     </script>

    <script>
        var comCheck;
        var pdfurl = @Html.Raw(Json.Encode(ViewBag.pdfurl));
        var pageno = @Html.Raw(Json.Encode(ViewBag.pages));
        var stateofwindow = window.open('/userlearning/viewpdfpopup?pdfurl=' + pdfurl + '&pages=' + pageno + '&sp=1&ep=10', 'learningindex', 'channelmode=1,resizable=1,scrollbars=0,location=0,titlebar=0,fullscreen=1,height=900,width=' + 1366 + '  , left=' + 1366 + ', toolbar=0, menubar=0,status=0');

