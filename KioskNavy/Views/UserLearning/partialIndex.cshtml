<table class="table table-bordered" style="color:whitesmoke">
    
@foreach (var item in Model) {
    <tr>
        <td>
            <a onClick="theFunction('@item.startPage','@item.endPage');" class="a2" style="text-align:center;color:white;font-size:large;width:550px;height:auto; background-color:#006dcc;font-family:Tahoma;font-weight:bold;box-shadow:0px 0px 5px 3px rgba(0,0,0,0.2);text-align:left" href="#">
                <font style="text-align:left" >@item.IndexName</font> 
            </a>
            <a href="#" class="a2" style="text-align:center;color:white;font-size:large;height:auto; background-color:#006dcc;font-family:Tahoma;font-weight:bold;box-shadow:0px 0px 5px 3px rgba(0,0,0,0.2);text-align:left">  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp; <font style="text-align:right">@item.startPage</font></a>
